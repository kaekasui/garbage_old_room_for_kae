<h1>Listing admin_side_menus</h1>

<table>
  <tr>
    <th>Menu</th>
    <th>Title</th>
    <th>Position</th>
    <th>Disable</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_side_menus.each do |admin_side_menu| %>
  <tr>
    <td><%= admin_side_menu.menu_id %></td>
    <td><%= admin_side_menu.title %></td>
    <td><%= admin_side_menu.position %></td>
    <td><%= admin_side_menu.disable %></td>
    <td><%= link_to 'Show', admin_side_menu %></td>
    <td><%= link_to 'Edit', edit_admin_side_menu_path(admin_side_menu) %></td>
    <td><%= link_to 'Destroy', admin_side_menu, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

